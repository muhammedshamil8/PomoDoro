{
    "manifest_version": 3,
    "name": "PomoDoro Focus Timer",
    "version": "1.0.0",
    "description": "A beautiful and feature-rich Pomodoro timer to boost your productivity.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon.jpeg",   
        "48": "icons/icon.jpeg",   
        "128": "icons/icon.jpeg"           
      }
    },
    "permissions": [
      "notifications",
      "storage",
      "alarms"
    ],
    "icons": {
      "16": "icons/icon.jpeg",  
      "48": "icons/icon.jpeg", 
      "128": "icons/icon.jpeg"
    },
    "background": {
      "service_worker": "background.js"
    }
  }